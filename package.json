{
  "name": "MostG",
  "version": "0.0.0",
  "description": "A proposed boiler plate to demonstrate lazy loading and server side rendering with webpack",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --mode production",
    "prebuild:watch": "rimraf dist",
    "build:watch": "webpack --mode development --watch",
    "pregenerate-api": "rimraf ./src/server/api",
    "start:dev": "concurrently -r --kill-others \"npm run build:watch\" \"npm run run:dev\"",
    "run:dev": "wait-on dist/server/server.js && nodemon --inspect dist/server/server.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
